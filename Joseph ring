#include <iostream>	
#include <vector>
int main(){
	int N, M;
	cin >> N >> M;
	vector<int> out;
	out.push_back(((M - 1) % N) + 1);
	for (int i = 1; i<N; i++) {
		int last = (M - 1)%(N-i);
		for (int j = N - i + 1; j <= N; j++) {
			last = (last + M) % j;
		}
		out.push_back(last + 1);
	}
	int n = out.size();
	for (int i = 0; i<n - 1; i++) {
		cout << out[i] << " ";
	}
	cout << out[n - 1];
	return 0;
}
